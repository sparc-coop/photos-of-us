
@{
    Layout = null;
    ViewData["Title"] = "BulkEditModal";
}

<md-dialog class="newfolder-dialog">
    <md-dialog-content>
        <div class="col-md-12">
            <div class="pull-left">
                <h2 class="header-xs">Bulk Edit</h2>
            </div>
            <div class="pull-right">
                <div class="close" aria-label="Close" ng-click="close()"><i class="material-icons">close</i></div>
            </div>
        </div>
        <div class="col-md-12">
            <form method="POST" class="form">
                <div class="form-input">
                    <md-chips readonly="ctrl.readonly">
                        <md-chip-template ng-repeat="tag in tags">
                            <strong>{{tag}}</strong>
                        </md-chip-template>
                    </md-chips>
                </div>
                <div class="form-button">
                    <button class="button--yellow" ng-click="test()">Apply</button>
                </div>

            </form>
        </div>
    </md-dialog-content>
</md-dialog>


